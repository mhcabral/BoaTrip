<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/primary_dark"
    tools:context="com.example.mhcabral.boatrip.Activitys.BuscaOrigemActivity">

    <include
        android:id="@+id/tb_busca_origem"
        layout="@layout/toolbar_internal"/>

    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/listView_busca_origem"
        android:layout_below="@id/tb_busca_origem"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@color/gray"/>

</RelativeLayout>
