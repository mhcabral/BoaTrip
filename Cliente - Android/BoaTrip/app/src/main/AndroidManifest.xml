<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mhcabral.boatrip" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_boatrip_logo"
        android:label="@string/app_name"
        android:theme="@style/Theme.MyTheme" >
        <activity
            android:name=".Activitys.InicialActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenSize"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activitys.BaseNavegationDrawerActivity"
            android:label="@string/title_activity_base" >
        </activity>
        <activity
            android:name=".Activitys.BaseInternalActivity"
            android:label="@string/title_activity_base_internal" >
        </activity>
        <activity
            android:name=".Activitys.BuscaOrigemActivity"
            android:label="@string/title_activity_busca_origem" >
        </activity>
        <activity
            android:name=".Activitys.BuscaDestinoActivity"
            android:label="@string/title_activity_busca_destino" >
        </activity>
        <activity
            android:name=".Activitys.BuscaActivity"
            android:label="@string/title_activity_inicial" >
        </activity>
        <activity
            android:name=".Activitys.BuscaResultActivity"
            android:label="@string/title_activity_busca_result" >
        </activity>
        <activity
            android:name=".Activitys.LoginActivity"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name=".Activitys.CadastroUserActivity"
            android:label="@string/title_activity_cadastro_user" >
        </activity>
        <activity
            android:name=".Activitys.PromocaoActivity"
            android:label="@string/title_activity_promocao" >
        </activity>
    </application>

</manifest>
